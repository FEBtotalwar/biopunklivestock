<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef MayRequire="Ludeon.RimWorld.Biotech" ParentName="AnimalThingBase">
    <defName>FEB_Leadbelly</defName>
    <label>leadbelly cow</label>
    <description>A heavily modified breed of domestic cattle adapted to toxic environments. A specialized digestive tract in combination with hereditary gut mechanites help leadbellies reduce ambient toxicity around them. Absorbed chemicals are then regurgitated and compacted, producing small amounts of toxglass renowned for it's beauty. n/n/ Unfortunately, the diet of leadbellies is limited to live plants that grow in polluted areas. Handlers working with these animals have a small chance to catch gut mechanites.</description>
    <statBases>
      <MoveSpeed>3.2</MoveSpeed>
      <ComfyTemperatureMin>-10</ComfyTemperatureMin>
      <MarketValue>550</MarketValue>
      <FilthRate>16</FilthRate>
	  <ToxicEnvironmentResistance>1</ToxicEnvironmentResistance>
      <Wildness>0.3</Wildness>
    </statBases>
    <comps>
      <li Class="CompProperties_Milkable">
        <milkDef>Milk</milkDef>
        <milkIntervalDays>1</milkIntervalDays>
        <milkAmount>7</milkAmount>
      </li>
	  <li Class="CompProperties_PollutionPump">
        <radius>5</radius>
        <pumpsPerWastepack>0</pumpsPerWastepack>
        <intervalTicks>120000</intervalTicks>
		<pumpEffecterDef>FEB_LeadbellyCleanup</pumpEffecterDef>
      </li>
	  <li Class="VEF.AnimalBehaviours.CompProperties_AnimalProduct">
		<resourceDef>FEB_Toxglass</resourceDef>
		<gatheringIntervalDays>2</gatheringIntervalDays>
		<resourceAmount>15</resourceAmount>
		<customResourceString>FEB_ToxglassDesc</customResourceString>
	  </li>
	  <li Class="VEF.AnimalBehaviours.CompProperties_EatWeirdFood">
		<customThingToEat>					
			<li>Plant_GrayGrass</li>
			<li>Plant_Ripthorn</li>
			<li>Plant_PebbleCactus</li>
			<li>Plant_Toxipotato</li>
			<li>FEB_Plant_Wiltgrass</li>
			<li MayRequire="Kierannkkt.MoreToxplants">Plant_ToxCotton</li>
			<li MayRequire="Kierannkkt.MoreToxplants">Plant_Toxleaf</li>
			<li MayRequire="Kierannkkt.MoreToxplants">Plant_Toxroot</li>
			<li MayRequire="Kierannkkt.MoreToxplants">Plant_Toxhops</li>
			<li MayRequire="Kierannkkt.MoreToxplants">Plant_Toxtoria</li>
			<li MayRequire="InertialMage.ToxicPlants">Plant_Toxiroot</li>
			<li MayRequire="basilthebatlord.toxicityevolved">Plant_Toxic_Haygrass</li>
			<li MayRequire="basilthebatlord.toxicityevolved">Plant_Toxilith_Bulb</li>
			<li MayRequire="basilthebatlord.toxicityevolved">Plant_Toxoid</li>
			<li MayRequire="basilthebatlord.toxicityevolved">Plant_Toxicotton</li>
			<li MayRequire="Neronix17.Outland.ToxicForest">Outland_PlantToxbeanRoot</li>
			<li MayRequire="Neronix17.Outland.ToxicForest">Outland_PlantToxplum</li>
			<li MayRequire="Neronix17.Outland.ToxicForest">Outland_PlantToxmelon</li>
			<li MayRequire="Neronix17.Outland.ToxicForest">Outland_PlantToxstalks</li>
			<li MayRequire="Neronix17.Outland.ToxicForest">Outland_PlantToxweed</li>
			<li MayRequire="Neronix17.Outland.ToxicForest">Outland_PlantTallToxstalks</li>
			<li MayRequire="Neronix17.Outland.ToxicForest">Outland_PlantToxpoms</li>
			<li MayRequire="emitbreaker.MIM.WH40k.Tox.Core">EMWH_Plant_HealrootToxic</li>
			<li MayRequire="emitbreaker.MIM.WH40k.Tox.Core">EMWH_Plant_ToxTinctoria</li>
			<li MayRequire="emitbreaker.MIM.WH40k.Tox.Core">EMWH_Plant_toxiberry</li>
			<li MayRequire="emitbreaker.MIM.WH40k.Tox.Core">EMWH_Plant_ToxAgave</li>
		</customThingToEat>
		<nutrition>1</nutrition>
		<areFoodSourcesPlants>true</areFoodSourcesPlants>
		<fullyDestroyThing>true</fullyDestroyThing>
		<needsWater>true</needsWater>
	  </li>
	  <li Class="VEF.AnimalBehaviours.CompProperties_HediffEffecter">
		<radius>1</radius>
		<severity>1</severity>
		<tickInterval>60000</tickInterval>
		<hediff>FEB_GutMechanites</hediff>
	  </li>
    </comps>
    <race>
	  <thinkTreeMain>VEF_AnimalWeirdEater</thinkTreeMain>
      <body>QuadrupedAnimalWithHooves</body>
      <herdAnimal>true</herdAnimal>
      <baseBodySize>2.4</baseBodySize>
      <baseHungerRate>0.535</baseHungerRate>
      <baseHealthScale>1.5</baseHealthScale>
      <foodType>VegetarianRoughAnimal</foodType>
      <leatherDef>VAEWaste_ToxicLeather</leatherDef>
	  <specificMeatDef>VAEWaste_ToxicMeat</specificMeatDef>
      <roamMtbDays>2</roamMtbDays>
      <trainability>None</trainability>
      <gestationPeriodDays>6.66</gestationPeriodDays>
      <lifeExpectancy>22</lifeExpectancy>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>0.20</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>0.3333</minAge>
          <soundWounded>Pawn_Muffalo_Wounded</soundWounded>
          <soundDeath>Pawn_Muffalo_Death</soundDeath>
          <soundCall>Pawn_Muffalo_Call</soundCall>
          <soundAngry>Pawn_Muffalo_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
      <soundEating>Herbivore_Eat</soundEating>
      <headPosPerRotation>
        <li>(0.0, 0, 0.09)</li>
        <li>(0.28, 0, 0)</li>
        <li>(0.0, 0, -0.04)</li>
        <li>(-0.28, 0, 0)</li>
      </headPosPerRotation>
    </race>
    <tradeTags>
      <li>AnimalCommon</li>
      <li>AnimalFarm</li>
    </tradeTags>
    <tools>
      <li>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>8.8</power>
        <cooldownTime>2.6</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
        <chanceFactor>0.7</chanceFactor>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>7</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
      <li>
        <label>left hoof</label>
        <labelNoLocation>hoof</labelNoLocation>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>8</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>FrontLeftLeg</linkedBodyPartsGroup>
      </li>
      <li>
        <label>right hoof</label>
        <labelNoLocation>hoof</labelNoLocation>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>8</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>FrontRightLeg</linkedBodyPartsGroup>
      </li>
    </tools>
  </ThingDef>

